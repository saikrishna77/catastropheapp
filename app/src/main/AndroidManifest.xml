<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sreet.learning">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TimeTable"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".Notices"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".activities"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".links"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".suggestions"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".timetabledisplay"
            android:parentActivityName=".TimeTable" />
        <activity android:name=".testfirebase"
            android:parentActivityName=".TimeTable"/>
        <activity android:name=".teststorage" />
        <activity android:name=".listViewClick"
            android:parentActivityName=".testfirebase"></activity>
    </application>

</manifest>